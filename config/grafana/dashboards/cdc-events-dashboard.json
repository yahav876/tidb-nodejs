      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right",
          "showLegend": true,
          "values": ["value", "percent"]
        },
        "pieType": "pie",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "displayLabels": ["name", "percent"]
      },
      "pluginVersion": "10.1.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          },
          "expr": "sum by (operation_type) (increase(tidb_cdc_events_total{operation_type=~\"insert|update|delete\"}[1h]))",
          "format": "time_series",
          "instant": true,
          "legendFormat": "{{operation_type}}",
          "refId": "A"
        }
      ],
      "title": "CDC Operations (Last Hour) - From Prometheus",
      "type": "piechart"
    },
    {
      "datasource": {
        "type": "elasticsearch",
        "uid": "Elasticsearch"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false,
            "filterable": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "@timestamp"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 180
              },
              {
                "id": "displayName",
                "value": "Timestamp"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "operation_type"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 120
              },
              {
                "id": "displayName",
                "value": "Operation"
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background",
                  "mode": "basic"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "INSERT": {
                        "text": "INSERT",
                        "color": "green"
                      },
                      "UPDATE": {
                        "text": "UPDATE",
                        "color": "blue"
                      },
                      "DELETE": {
                        "text": "DELETE",
                        "color": "red"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "table_name"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Table"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "data.id"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 80
              },
              {
                "id": "displayName",
                "value": "Record ID"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "data.username"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Username"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "data.email"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Email"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "id": 1,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "@timestamp"
          }
        ]
      },
      "pluginVersion": "10.1.0",
      "targets": [
        {
          "datasource": {
            "type": "elasticsearch",
            "uid": "Elasticsearch"
          },
          "query": "message:\"CDC Event\" AND operation_type:*",
          "refId": "A",
          "bucketAggs": [],
          "metrics": [
            {
              "id": "1",
              "type": "raw_document",
              "settings": {
                "size": "500"
              }
            }
          ],
          "timeField": "@timestamp"
        }
      ],
      "title": "Raw CDC Events List - From Elasticsearch",
      "type": "table",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "_id": true,
              "_index": true,
              "_type": true,
              "agent": true,
              "app_service": true,
              "ecs": true,
              "host": true,
              "input": true,
              "key": true,
              "level": true,
              "log": true,
              "log_source": true,
              "message": true,
              "offset": true,
              "sort": true,
              "timestamp": true,
              "highlight": true
            },
            "indexByName": {
              "@timestamp": 0,
              "operation_type": 1,
              "table_name": 2,
              "data.id": 3,
              "data.username": 4,
              "data.email": 5,
              "data.created_at": 6,
              "data.updated_at": 7
            },
            "renameByName": {}
          }
        }
      ]
    }
  ],
